import java.util.*;;
class Solution
{
    public static void main(String args[])
    {
        Scanner scan=new Scanner(System.in);
        int i,n;
        n=scan.nextInt();
        for(i=1;i<11;i++)
        {
            System.out.println(n+" x "+i+ " = " +n*i);
        }
        
        
        
        
        scan.close();
    }
}
