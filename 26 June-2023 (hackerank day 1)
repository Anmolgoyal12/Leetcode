//Day 1
import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
	
    public static void main(String[] args) {
        int i = 4;
        double d = 4.0;
        String s = "HackerRank ";
		
        Scanner scan = new Scanner(System.in);

        /* Declare second integer, double, and String variables. */
int a;
double a1;
String a2;


a=scan.nextInt();
a1=scan.nextDouble();
scan.nextLine();
a2=scan.nextLine();
System.out.println(i+a);
System.out.println(d+a1);
System.out.println(s.concat(a2));


        scan.close();
    }
}
